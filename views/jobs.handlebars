<section id="gigs" class="container">
    <h1>All Jobs</h1>

    {{#each jobs}}
    <div class="gig">
        <h2>{{title}}</h2>
        <p>{{description}}</p>
        <ul>
            <li>Budget: {{budget}}</li>
            <li>
                <a href="#" class="btn btn-reverse">Apply Now</a>
            </li>
        </ul>
        <div class="tech">
            <small>Technologies Needed:
                <span>{{technologies}}</span>
            </small>
        </div>
    </div>
    {{else}}
        <p>No Jobs available</p>
     {{/each}}
    

</section>